<div class="new">
  <h1>Login</h1>

<div class="form">

<%= form_for :session, url: :sessions do |f| %>

    <h4><%= f.label :email %> </h4>
    <h4><%= f.email_field :email %> </h4>

    <h4><%= f.label :password %> </h4>
    <h4><%= f.password_field :password %> </h4>

    </h4><%= f.submit 'Login' %> </h4>

<% end %>

</div>
</div>
